        imp=sbruto*0.45;//  45 % impuesto
        
        sneto=sbruto-imp;
    }
        
    printf("su salario bruto es %.2f.\nsu salario neto es de%.2f.\nsu tasa de impuestos es de %.2f.",sbruto,sneto,imp);
    
}
